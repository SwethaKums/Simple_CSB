<%= colleges ||=[] %>
<ul>
	<% colleges.each do |college| %>
	<li>
		<%= college.college_name %>
		<ul>
			<% college.students.ascending.each do |student| %>
			<li><%=link_to_unless_current(student.student_name, public_show_path(student.permalink))%>
			</li>
			<% end %>
		</ul>
	</li>
	<% end %>
</ul>
